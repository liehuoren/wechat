/* pages/user/address/list/list.wxss */
.address-list {
	padding-bottom: 61px;	
  overflow: hidden;
}

.weui-panel:before, .weui-panel:after {
  border: none;
}
.weui-media-box__info::before {
  border: 1px solid #999;
}
.icon-check {
  width: 30rpx;
  height: 30rpx;
}
.icon {
  width: 35rpx;
  height: 35rpx;
  margin-right: 15rpx;
}
.right {
  float: right;
}
.text-orangered {
  font-size: 14px;
  color: #666;
}
.tool {
  padding-left: 1em;
  padding-right: 0;
}
.empty {
  padding-top: 60px;
  text-align: center;
}
.empty image {
  width: 270rpx;
  height: 286rpx;
}
.empty .title {
  margin-top: 10px;
}
.empty .desc {
  font-size: 12px;
  color: #999;
}
.bottom-btn {
  position: fixed;
  bottom: 15px;
  width: 100%;
  height: 46px;
}
.add-btn {
  width: 90%;
  margin: 0 auto;
}