/**app.wxss**/
@import 'styles/weui.wxss';
@import 'styles/prompt.wxss';
@import 'styles/list.wxss';

page {
  width: 100%;
  height: 100%;
  background-color: #F8F8F8;
  font-size: 16px;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
button[type=primary] {
  background-color: #ffaa2b!important;
  border: 0px;
}
button-hover {
  background-color: #ff9f10!important;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}